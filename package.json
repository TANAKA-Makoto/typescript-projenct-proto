{
  "name": "typescript-projenct-proto",
  "version": "1.0.0",
  "description": "",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:watch": "ts-node-dev --respawn src/index.ts",
    "check-types": "tsc --noEmit",
    "eslint": "eslint src/**/*.ts",
    "eslint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write 'src/**/*.{js,ts,json}'",
    "lint": "npm-run-all eslint check-types",
    "lint:fix": "npm-run-all eslint:fix check-types format"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TANAKA-Makoto/typescript-projenct-proto.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TANAKA-Makoto/typescript-projenct-proto/issues"
  },
  "engines": {
    "node": ">= 12",
    "npm": ">= 6.14"
  },
  "homepage": "https://github.com/TANAKA-Makoto/typescript-projenct-proto#readme",
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.9",
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.3.0",
    "eslint": "8.46.0",
    "eslint-config-prettier": "9.0.0",
    "husky": "8.0.3",
    "jest": "^29.6.2",
    "lint-staged": "13.2.3",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.1",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "^5.1.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,ts}": "eslint --cache --fix",
    "*.ts": "tsc --noEmit",
    "*.{js,ts,json}": "prettier --write"
  }
}
